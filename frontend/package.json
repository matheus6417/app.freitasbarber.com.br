{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:localhost",
    "start:localhost": "vue-cli-service serve --mode localhost",
    "build:localhost": "vue-cli-service build --mode localhost",
    "start:development": "vue-cli-service serve --mode development",
    "build:development": "vue-cli-service build --mode development",
    "deploy:development": "firebase use development && npm run build:development && firebase deploy --only hosting,storage",
    "start:production": "vue-cli-service serve --mode production",
    "build:production": "vue-cli-service build --mode production",
    "deploy:production": "firebase use production && npm run build:production && firebase deploy --only hosting,storage",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apollo-boost": "0.3.1",
    "apollo-link-context": "1.0.14",
    "apollo-link-retry": "2.2.10",
    "chart.js": "2.7.3",
    "element-ui": "2.5.4",
    "file-saver": "2.0.1",
    "filesize": "4.1.2",
    "firebase": "5.8.4",
    "graphql": "14.1.1",
    "lodash": "4.17.11",
    "md5": "2.2.1",
    "moment": "2.24.0",
    "nprogress": "0.2.0",
    "portal-vue": "2.1.6",
    "uuid": "3.3.2",
    "vue": "2.6.7",
    "vue-router": "3.0.2",
    "vuex": "3.1.0",
    "xlsx": "0.14.1",
    "yup": "0.26.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.4.1",
    "@vue/cli-plugin-eslint": "3.4.1",
    "@vue/cli-service": "3.4.1",
    "@vue/eslint-config-prettier": "4.0.1",
    "babel-eslint": "10.0.1",
    "eslint": "5.14.1",
    "eslint-plugin-vue": "5.2.2",
    "tailwindcss": "^1.1.2",
    "vue-cli-plugin-element": "1.0.1",
    "vue-template-compiler": "2.6.7"
  }
}
